<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Open Cart Test Regression Suite" verbose="3" thread-count="4" parallel="tests">

<listeners>
		<listener
			class-name="com.qa.opencart.listeners.ExtentReportListener" />
		

	</listeners>


	<test name="Login Page Test">
       <classes>
			<class name="com.qa.opencart.tests.LoginPageTest" />
		</classes>
	</test>
	
	<test name="Account Page Test">
       <classes>
			<class name="com.qa.opencart.tests.AccountPageTest" />
		</classes>
	</test>
	
	<test name="ProductsResult Page Test">
       <classes>
			<class name="com.qa.opencart.tests.ProductsResultPageTest" />
		</classes>
	</test>
	
	<test name="Register Page Test">
       <classes>
			<class name="com.qa.opencart.tests.RegisterPageTest" />
		</classes>
	</test>
	
</suite>